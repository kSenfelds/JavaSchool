
public class Testmass {

	public static void main(String[] args) {
		int [] x = {5,4,3,2,1};
		int nul=-1 ;
		int one=-1;
		int sk=1;
		do { if (x[sk-1]>x[sk]) {nul = x[sk-1];one = x[sk];}
			x[sk-1]=nul;
			x[sk]=one;
			sk++;
			
		}while (sk<x.length);
		for (int i = 0; i < x.length; i++) {
			System.out.print(x[i]);
		}
			
	}

}
